{
  "name": "agda-mode",
  "main": "./lib/agda-mode",
  "version": "0.6.14",
  "description": "agda-mode on Atom",
  "activationCommands": {
    "atom-text-editor[data-grammar='source agda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ],
    "atom-text-editor[data-grammar='source lagda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ]
  },
  "atomTestRunner": "atom-mocha",
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">0.180.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/classnames": "^2.2.0",
    "@types/electron": "^1.6.10",
    "@types/lodash": "^4.14.70",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.14",
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.46",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^1.2.6",
    "@types/semver": "^5.3.32",
    "bluebird": "^3.5.0",
    "classnames": "^2.2.5",
    "duplexer": "^0.1.1",
    "lodash": "^4.17.4",
    "parsimmon": "^1.6.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "atom-mocha": "^2.0.1",
    "atom-mocha-test-runner": "^1.0.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "mocha": "^3.4.2",
    "temp": "^0.8.3"
  }
}
