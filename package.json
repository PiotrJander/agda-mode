{
  "name": "agda-mode",
  "main": "./lib/agda-mode",
  "version": "0.6.14",
  "description": "agda-mode on Atom",
  "activationCommands": {
    "atom-text-editor[data-grammar='source agda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ],
    "atom-text-editor[data-grammar='source lagda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ]
  },
  "atomTestRunner": "./lib/test/setup",
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">0.180.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.3",
    "@types/classnames": "0.0.32",
    "@types/electron": "^1.4.37",
    "@types/lodash": "^4.14.63",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.14",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.40",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^1.2.4",
    "@types/semver": "^5.3.31",
    "bluebird": "3.5.0",
    "classnames": "2.2.5",
    "duplexer": "^0.1.1",
    "lodash": "^4.17.4",
    "parsimmon": "1.0.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-actions": "0.11.0",
    "redux-thunk": "^2.2.0",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "atom-mocha": "1.0.0",
    "atom-mocha-test-runner": "0.3.0",
    "chai": "3.4.1",
    "chai-as-promised": "5.1.0",
    "chai-things": "0.2.0",
    "mocha": "2.3.4",
    "temp": "0.8.3"
  }
}
